{
    "name": "Tab Title Fetcher",
    "description": "Fetch the title of the current tab and display 'chrome extension (1)'.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "/images/ext1.png",
      "32": "/images/ext2.png",
      "48": "/images/ext3.png"
    }
  }
  